{
  "name": "vue-femtotween",
  "version": "0.1.2",
  "description": "Vue 3 Composition API reactivity wrapper for femtoTween",
  "files": [
    "dist",
    "index.ts",
    "index.d.ts"
  ],
  "main": "dist/umd.js",
  "module": "dist/es.js",
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/umd.js"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc --noEmit && vite build",
    "clean": "rimraf ./dist",
    "prepublish": "npm run build",
    "test": "echo \"No test specified\" && exit 0"
  },
  "keywords": [
    "vue",
    "femtotween",
    "tween",
    "tweening"
  ],
  "author": "Nicholas Sidwell <nicholas@sidwell.dev>",
  "license": "MIT",
  "repository": "https://github.com/euphemism/vue-femtotween",
  "dependencies": {
    "femtotween": "^2.0.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "vite": "^2.8.6"
  }
}
